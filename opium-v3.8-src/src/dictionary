rvxc[i] : exchange correlation potential at radius i
rexc[i] : exchange correlation energy density at radius i

rsatom[i] : density at radius i

rnl[i,j] : wavefunction at radius i of orbital j

npdm : size of radial grid ?
n0 : number of orbitals ?

no(m) : n-quantum number of orbital m ?
lo(m) : l-quantum number of orbital m ?
en(m) : energy of orbital m ?

subroutine schsl(iorb,nqn,lang,ei,imax,rv,p,igh,ibound)
  iorb : orbital index
  nqn  : n-quantum number of orbital iorb ?
  lang : l-quantum number of orbital iorb ?
  ei : energy of orbital iorb ?
  imax : ?
  rv : some potential ?
  p : output wavefunction 
  igh : ?
  ibound : ?

subroutine hfmethd(m,ipsp,p,zeff,azz,fail,iprint)
  m : orbital index
  ipsp : ?
  p : output wavefunction
  zeff = z - s, where s = screening parameter
  azz : ?
  fail : .true. if the calculation fails
  iprint : ?
  

