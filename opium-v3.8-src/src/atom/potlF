hfint.F:      write(*,*) "before potl"
hfint.F:      call potl(yr,m)
hfint.F:      write(*,*) "after potl"
hfsolve.F:c            call potl(yr,i)
hfsolve.F:            call potl(yr,i)
hfuncs.F:      call potl(yr,i)
